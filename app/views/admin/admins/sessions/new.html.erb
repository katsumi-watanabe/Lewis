<div class="wrapper">
  <%= form_for(resource, as: resource_name, html: { class: "form-signin" }, url: session_path(resource_name)) do |f| %>
    <div class="div-bottom">
    <%= link_to admins_guest_sign_in_path, method: :post do %>
      <button class="btn btn-lg btn-block gest-log"  name="Submit" value="Login" type="Submit">管理者ゲストログイン</button>
    <% end %>
    </div>
    <hr class="colorgraph"><br>
    <div>
      <%= f.email_field :email, autofocus: true, autocomplete: "email", class: "form-control", placeholder: "メールアドレス"  %>
      <%= f.password_field :password, autocomplete: "current-password", class: "form-control", placeholder: "パスワード"  %>
    </div>
    <div class="actions div-bottom">
      <%= f.submit "ログイン", class: "log log-in btn btn-lg btn-block" %>
    </div>
    <div class="mt-2">
      <%= link_to new_user_session_path, class: "text-primary mt-2" do  %>
        <p class="form-signin-heading admin-log-here">ユーザーのログインはこちら<i class="fas fa-sign-in-alt"></i></p>
      <% end %>
    </div>
	<% end %>
</div>